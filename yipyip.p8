--appa (player)

function makeplayer()
p={}

--sets starting position for appa
p.x = 5  
p.y = 1
--

--char sprite numbers
p.spriter=1
p.spritel=5

p.sprite2=2  --top of jump appa
p.spritej = 18 --bottom of shadow
--p.sprited = 49

--p.coins=0
--direction = 3

end

--curenttly draws appa and
--jumping appa to screen
function drawappa(direct) --direction parameter will be added

if(direct == 0) then
  spr(p.spritel,p.x*8,p.y*8)

elseif(direct == 1) then
 spr(p.spriter,p.x*8,p.y*8)
else 
	spr(p.sprite2,(p.x)*8,p.y*8)
 spr(p.spritej,(p.x)*8,(p.y+1)*8)
end 

--jumping appa sprite here
--spr(p.sprite2,(p.x+2)*8,p.y*8)
--spr(p.spritej,(p.x+2)*8,(p.y+1)*8)
end

--movement function
function move_appa()
tox=p.x
toy=p.y
--0 = left 1 == right
 

if (btnp(⬅️)) then
 tox -=1
	lastdir = 0
	end
if (btnp(➡️)) then
 tox +=1
 lastdir = 1 
 end
if (btnp(⬆️)) then
 toy -=1
 lastdir = 2
 end
if (btnp(⬇️)) then
 toy +=1
 lastdir = 3
end

--if can move
 --set new appa position
 p.x=mid(0,tox,127)
	p.y=mid(0,toy,63)


 return lastdir
end
