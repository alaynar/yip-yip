--appa (player)

function makeplayer()
p={}

--sets starting position for appa
p.x = 5  
p.y = 1
--

--char sprite numbers
p.sprite=1
p.sprite2=2  --top of jump appa
p.spritej = 18 --bottom of shadow
--p.sprited = 49

--p.coins=0
--direction = 3

end

--curenttly draws appa and
--jumping appa to screen
function drawappa() --direction parameter will be added
spr(p.sprite,p.x*8,p.y*8)
spr(p.sprite2,(p.x+2)*8,p.y*8)
spr(p.spritej,(p.x+2)*8,(p.y+1)*8)
end
