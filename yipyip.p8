--movement function
function move_appa()
	tox=p.x
	toy=p.y

 if (prevdir=='u') then
   if (btnp(‚¨ÖÔ∏è)) then
 	  tox -=1
		  lastdir = 4
	  end
	  if (btnp(‚û°Ô∏è)) then
 	  tox +=1
 	  lastdir = 5 
   end
 	 if (btnp(‚¨áÔ∏è)) then
 	   toy +=1
 	   lastdir = 3
 	   prevdir = 'd'
 	 end
 	 if (btnp(‚¨ÜÔ∏è)) then
 	   toy -=1
 	   lastdir = 2
 	 end
 elseif (prevdir=='d') then
   if (btnp(‚¨ÖÔ∏è)) then
 	  tox -=1
		  lastdir = 0
	  end
	  if (btnp(‚û°Ô∏è)) then
 	  tox +=1
 	  lastdir = 1 
   end
   if (btnp(‚¨ÜÔ∏è)) then
    toy -=1
    lastdir = 2
    prevdir = 'u'
   end
   if (btnp(‚¨áÔ∏è)) then 
    toy +=1
    lastdir = 3
   end 
	end

--checks for textbox
 interaction_text(tox,toy)
 
-- checks if the player can move 
 if (can_move(tox,toy)) then
 	p.x=mid(0,tox,127)
		p.y=mid(0,toy,63)
 else if (not can_fly(tox,toy)) then
  sfx(0)
 else
 end
 end
 
 if (is_powerup(tox,toy)) then
 		timerdur = timer + 300
 		swap_tile(tox,toy,16)
 		sfx(0)
 end

 if (can_fly(tox,toy) and (jumping == 1)) then
  p.x=mid(0,tox,127)
		p.y=mid(0,toy,63)
 end
 
 dset(0, p.x)
 dset(1, p.y)
  
 return lastdir
end
